{
  "name": "express-skeleton",
  "version": "0.0.1",
  "author": {
    "name": "Mochamad zulfikar",
    "email": "m.zulfikar@rebelworks.co"
  },
  "license": "MIT",
  "description": "express-skeleton",
  "main": "src/index.ts",
  "scripts": {
    "prestart": "npm run build",
    "start": "node -r ./tspaths.js ./dist/src/index.js",
    "start:dev": "nodemon -w src -e ts -r tsconfig-paths/register --exec ts-node ./src/index.ts",
    "start:test-server": "nodemon --exitcrash -w src -e ts -r tsconfig-paths/register --exec ts-node ./src/index.ts",
    "dev": "concurrently \"npm run tsoa:generate_\" \"npm run start:dev\"",
    "prebuild": "rimraf ./build && npm run tsoa:generate",
    "build": "tsc",
    "tsoa:generate_": "nodemon -w src -w prisma -e ts _generate-tsoa.ts",
    "tsoa:generate": "ts-node _generate-tsoa.ts",
    "test": "NODE_ENV=test node runtest.js",
    "db:seed": "ts-node -r tsconfig-paths/register ./utils/seeder.ts",
    "eslint": "eslint --ext .ts --fix ./src -c .eslintrc.js",
    "watch:test": "yarn jest --watch",
    "generate": "openapi --input ./tsoa/swagger.json --output ./generated"
  },
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "_moduleAliases": {
    "@database": "./build/src/database/index.ts",
    "@entity": "./build/src/database/entity/"
  },
  "keywords": [
    "express",
    "typescript"
  ],
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@ngneat/falso": "^6.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.5",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/morgan": "^1.9.3",
    "@types/node": "^14.18.21",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^4.33.0",
    "child_process": "^1.0.2",
    "concurrently": "^6.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^28.1.3",
    "nodemon": "^1.3.3",
    "openapi-typescript-codegen": "^0.23.0",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "test-jest": "^1.0.1",
    "ts-jest": "^28.0.6",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "*",
    "typescript-bundle": "^1.0.18",
    "wait-on": "^6.0.1"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "env-cmd": "^10.1.0",
    "express": "^4.18.1",
    "fs": "^0.0.1-security",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "require-json5": "^1.1.0",
    "sqlite3": "^5.0.2",
    "swagger-ui-express": "^4.1.6",
    "tsconfig-paths": "^3.9.0",
    "tsoa": "^3.5.2",
    "typeorm": "^0.2.31",
    "typeorm-extension": "^2.1.4",
    "typeorm-seeding": "^1.6.1"
  }
}
